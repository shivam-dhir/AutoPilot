{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["ES2015", "DOM"],
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@fixtures/*": ["src/fixtures/*"],
			"@model/*": ["src/model/*"],
			"@pages/*": ["src/pages/*"],
			"@pagecomponents/*": ["src/pagecomponents/*"],
			"@queries/*": ["src/data/queries/*"],
			"@data-factory/*": ["src/data/*"]
    }
  }
}
